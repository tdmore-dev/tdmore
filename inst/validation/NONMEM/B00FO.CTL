$PROBLEM THEOPHYLLINE ORAL METHOD=ZERO POSTHOC
$INPUT ID AMT TIME DV BWT
$DATA THEO
$PRED
  DOSE = 320
  KA   = THETA(1) * EXP(ETA(1))
  V    = THETA(2) * EXP(ETA(2))
  K    = THETA(3) * EXP(ETA(3))

  F    = DOSE / V * KA / (KA - K) * (EXP(-K*TIME) - EXP(-KA*TIME))
  Y    = F + F*EPS(1) + EPS(2)

  IF (ICALL.EQ.3) THEN
    WRITE (52,*) THETA
    WRITE (53,*) OMEGA(BLOCK)
    WRITE (54,*) SIGMA(BLOCK)
  ENDIF

$THETA
  (0, 2)
  (0, 50)
  (0, 0.1)

$OMEGA BLOCK(3)
  0.2
  0.1  0.2
  0.1  0.1  0.2

$SIGMA
  0.1
  0.1

$ESTIMATION MAXEVALS=9999 METHOD=ZERO POSTHOC
$COVARIANCE
$TABLE ID ETA(1) ETA(2) ETA(3)
       FILE=B00FO.ETA NOAPPEND FIRSTONLY ONEHEADER
