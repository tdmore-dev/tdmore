$PROBLEM PHENOBARBITAL IV METHOD=ZERO POSTHOC
$INPUT ID TIME AMT BWT APGR DV MDV EVID
$DATA  PHENO IGNORE=@
$SUBROUTINES ADVAN1
$ABBREVIATED COMRES=20
$PK
  TVV  = THETA(1) + (BWT/1.5)**THETA(2)
  TVK  = THETA(3)
  V    = TVV * EXP(ETA(1))
  K    = TVK * EXP(ETA(2))
  S1   = V

$ERROR
  IPRE  = F
  Y     = F + F*EPS(1) + EPS(2)

  IF (ICALL.EQ.3) THEN
    WRITE (52,*) THETA
    WRITE (53,*) OMEGA(BLOCK)
    WRITE (54,*) SIGMA(BLOCK)
  ENDIF

"LAST
" COM(1) = G(1,1)
" COM(2) = G(2,1)
" COM(3) = HH(1,1)
" COM(4) = HH(2,1)

$THETA
  (0, 1)
  (0, 1)
  (0, 0.01)

$OMEGA BLOCK(2)
  0.2
  0.1  0.2

$SIGMA
  0.1
  3

$ESTIMATION MAXEVAL=9999 PRINT=5 METHOD=ZERO POSTHOC
$COVARIANCE
$TABLE ID TIME AMT MDV IPRE BWT COM(1)=G11 COM(2)=G21 COM(3)=H11 COM(4)=H21
       FILE=C00FO.DER NOPRINT ONEHEADER 
$TABLE ID ETA(1) ETA(2)
       FILE=C00FO.ETA ONEHEADER NOAPPEND FIRSTONLY

